{
  "name": "ICQueue",
  "version": "7.0.4-1",
  "engines": {
    "node": ">= 8"
  },
  "description": "Dead simple consume & publish for RabbitMQ.",
  "main": "icqueue.js",
  "scripts": {
    "coverage": "nyc -a -c -r html -r text -r lcov npm test",
    "docs": "jsdoc2md icqueue.js",
    "test": "semistandard &&set NODE_ENV=test&& mocha test --timeout 10000 --recursive --exit"
  },
  "repository": {
    "type": "git",
    "url": "http://github/pear/ICQueue"
  },
  "keywords": [
    "AMQP",
    "AMQP 0-9-1",
    "RabbitMQ",
    "amqplib",
    "consume",
    "publish",
    "icq",
    "icqueue"
  ],
  "dependencies": {
    "amqplib": "^0.5.6",
    "debug": "^2.6.8",
    "json-stringify-safe": "^5.0.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "dirty-chai": "^2.0.1",
    "jsdoc-to-markdown": "^5.0.0",
    "mocha": "^6.2.0",
    "nyc": "^11.4.1",
    "sandboxed-module": "^0.3.0",
    "semistandard": "^14.1.0",
    "sinon": "^7.3.2",
    "sinon-chai": "^3.3.0"
  },
  "semistandard": {
    "env": [
      "mocha"
    ]
  }
}
