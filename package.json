    "debug": "^2.6.8",
    "json-stringify-safe": "^5.0.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "dirty-chai": "^2.0.1",
    "jsdoc-to-markdown": "^5.0.0",
    "mocha": "^6.2.0",
    "nyc": "^11.4.1",
    "sandboxed-module": "^0.3.0",
    "semistandard": "^14.1.0",
    "sinon": "^7.3.2",
